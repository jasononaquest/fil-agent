# ===========================================
# Deployment Configuration
# ===========================================
# Copy this file to .deploy.env and fill in your values:
#   cp .deploy.env.example .deploy.env
#
# IMPORTANT: .deploy.env contains secrets and should NOT be committed to git

# GCP Project Configuration
PROJECT_ID=your-gcp-project-id
PROJECT_NUMBER=123456789012
REGION=us-west1
STAGING_BUCKET=gs://your-staging-bucket

# MCP Server URL (Cloud Run deployment)
MCP_SERVER_URL=https://your-mcp-server.run.app/sse

# Rails Internal API (for real-time event push)
RAILS_EVENTS_URL=https://your-rails-app.com/api/internal/agent_events
INTERNAL_API_TOKEN=your-internal-api-token

# Current deployed agent ID (for test_deployed_agent.py)
# Run ./list-agents.sh to see available agents
AGENT_ID=
