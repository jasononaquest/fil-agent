# Google AI / Vertex AI Configuration
# For local development with API key:
GOOGLE_GENAI_USE_VERTEXAI=FALSE
GOOGLE_API_KEY=your-google-ai-api-key

# For production with Vertex AI:
# GOOGLE_GENAI_USE_VERTEXAI=TRUE
# GOOGLE_CLOUD_PROJECT=your-gcp-project
# GOOGLE_CLOUD_LOCATION=us-west1

# MCP Server Connection (required)
MCP_SERVER_URL=http://localhost:8000/sse

# Optional: API key for MCP server authentication
# MCP_API_KEY=your-mcp-api-key

# OpenTelemetry / Cloud Trace (for Agent Engine)
GOOGLE_CLOUD_AGENT_ENGINE_ENABLE_TELEMETRY=true
OTEL_INSTRUMENTATION_GENAI_CAPTURE_MESSAGE_CONTENT=true
